<%= form_for @object do |f| %>
  <table class="table table-striped table-bordered">
    <thead>
      <th>Url</th>
      <th>Link Type</th>
      <th>Link Description</th>
     </thead>
     <tbody>
       <% @object.ordered_links.each do |link| %>
         <tr>
           <td><%= link.url_text %></td>
           <td><%= link.url_type %></td>
           <td><%= link.description %></td>
         </tr>
       <% end %>
     </tbody>
  </table>
<% end %>
