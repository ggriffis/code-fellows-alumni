<%= form_for [@project] do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <p>
    <b><%= f.label :title %></b><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <b><%= f.label :description %></b><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <b>Attachment</b><br>
    <%= f.file_field :attach %>
  </p>
  <%= f.submit %>
<% end %>
